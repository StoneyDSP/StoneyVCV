{
  "version": 4,
  "env": {
    "RACK_DIR": ["${RACK_DIR}"]
  },
  "configurations": [
    {
      "name": "Linux",
      "includePath": [
        "${default}"
      ],
      "defines": [
        "${default}"
      ],
      "compilerArgs": [
        "-std=c++11",
        "-Wsuggest-override",
        "-fPIC",
        "-I${workspaceFolder}/include",
        "-I${workspaceFolder}/dep/StoneyDSP/include",
        "-I${workspaceFolder}/dep/Catch2/include",
        "-I{RACK_DIR}/include",
        "-I${RACK_DIR}/dep/include",
        "-fno-gnu-unique",
        "-MMD",
        "-MP",
        "-g",
        "-O3",
        "-funsafe-math-optimizations",
        "-fno-omit-frame-pointer",
        "-Wall",
        "-Wextra",
        "-Wno-unused-parameter",
        "-march=nehalem",
        "-L${RACK_DIR}",
        "-lRack",
        "-static-libstdc++",
        "-static-libgcc"
      ],
      "browse": {
        "databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db",
        "limitSymbolsToIncludedHeaders": true,
        "path": [
          "${default}"
        ]
      },
      "compilerPath": "/usr/bin/g++",
      "intelliSenseMode": "linux-gcc-x64",
      "cStandard": "gnu11",
      "cppStandard": "gnu++11",
      "configurationProvider": "ms-vscode.cmake-tools",
      "mergeConfigurations": true,
      "compileCommands": "${workspaceFolder}/build/compile_commands.json"
    },
    {
      "name": "Mac",
      "includePath": [
        "${default}"
      ],
      "defines": [
        "${default}"
      ],
      "compilerArgs": [
        "-std=c++11",
        "-stdlib=libc++",
        "-fPIC",
        "-I${workspaceFolder}/include",
        "-I${workspaceFolder}/dep/StoneyDSP/include",
        "-I${workspaceFolder}/dep/Catch2/include",
        "-I{RACK_DIR}/include",
        "-I${RACK_DIR}/dep/include",
        "-MMD",
        "-MP",
        "-g",
        "-O3",
        "-funsafe-math-optimizations",
        "-fno-omit-frame-pointer",
        "-Wall",
        "-Wextra",
        "-Wno-unused-parameter",
        "-march=armv8-a+fp+simd",
        "-mmacosx-version-min=10.9",
        "-L${RACK_DIR}",
        "-lRack",
        "-undefined dynamic_lookup"
      ],
      "browse": {
        "databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db",
        "limitSymbolsToIncludedHeaders": true,
        "path": [
          "${default}"
        ]
      },
      "compilerPath": "c++",
      "intelliSenseMode": "macos-clang-arm64",
      "cStandard": "c11",
      "cppStandard": "c++11",
      "configurationProvider": "ms-vscode.cmake-tools",
      "mergeConfigurations": true,
      "compileCommands": "${workspaceFolder}/build/compile_commands.json"
    },
    {
      "name": "Windows",
      "includePath": [
        "${default}"
      ],
      "defines": [
        "${default}",
        "_USE_MATH_DEFINES"
      ],
      "compilerArgs": [
        "-std=c++11",
        "-Wsuggest-override",
        "-fPIC",
        "-I${workspaceFolder}\\include",
        "-I${workspaceFolder}\\dep\\StoneyDSP\\include",
        "-I${workspaceFolder}\\dep\\Catch2\\include",
        "-I{RACK_DIR}\\include",
        "-I${RACK_DIR}\\dep\\include",
        "-MMD",
        "-MP",
        "-g",
        "-O3",
        "-funsafe-math-optimizations",
        "-fno-omit-frame-pointer",
        "-Wall",
        "-Wextra",
        "-Wno-unused-parameter",
        "-march=nehalem",
        "-municode",
        "-L${RACK_DIR}",
        "-lRack",
        "-static-libstdc++"
      ],
      "browse": {
        "databaseFilename": "${workspaceFolder}\\.vscode\\browse.vc.db",
        "limitSymbolsToIncludedHeaders": true,
        "path": [
          "${default}"
        ]
      },
      "compilerPath": "C:\\msys64\\mingw64\\bin\\g++.exe",
      "intelliSenseMode": "windows-gcc-x64",
      "cStandard": "gnu11",
      "cppStandard": "gnu++11",
      "configurationProvider": "ms-vscode.cmake-tools",
      "mergeConfigurations": true,
      "compileCommands": "${workspaceFolder}\\build\\compile_commands.json"
    }
  ]
}
