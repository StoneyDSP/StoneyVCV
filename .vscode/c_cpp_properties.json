{
  "version": 4,
  "env": {
    "RACK_DIR": ["${RACK_DIR}"]
  },
  "configurations": [
    {
      "name": "Linux GNU",
      "includePath": [
        "${default}"
      ],
      "defines": [
        "STONEYDSP_VERSION_MINOR=1"
      ],
      "compilerArgs": [
        "-std=c++11",
        "-Wsuggest-override",
        "-fPIC",
        // "-Iinclude",
        // "-I{RACK_DIR}/include",
        // "-I${RACK_DIR}/dep/include",
        "-fno-gnu-unique",
        "-MMD",
        "-MP",
        "-g",
        "-O3",
        "-funsafe-math-optimizations",
        "-fno-omit-frame-pointer",
        "-Wall",
        "-Wextra",
        "-Wno-unused-parameter",
        "-march=nehalem"
        // "-L${RACK_DIR}",
        // "-lRack",
        // "-static-libstdc++",
        // "-static-libgcc"
      ],
      "browse": {
        "databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db",
        "limitSymbolsToIncludedHeaders": true,
        "path": [
          "${default}"
        ]
      },
      "compilerPath": "/usr/bin/g++",
      "intelliSenseMode": "linux-gcc-x64",
      "cStandard": "gnu11",
      "cppStandard": "gnu++11",
      "configurationProvider": "ms-vscode.makefile-tools",
      "mergeConfigurations": true
    },
    {
      "name": "Windows MSYS2 MinGW64",
      "includePath": [
        "${default}"
      ],
      "defines": [
        "${default}",
        "_USE_MATH_DEFINES"
      ],
      "compilerArgs": [
        "-std=c++11",
        "-Wsuggest-override",
        "-fPIC",
        // "-Iinclude",
        // "-I{RACK_DIR}\\include",
        // "-I${RACK_DIR}\\dep\\include",
        "-MMD",
        "-MP",
        "-g",
        "-O3",
        "-funsafe-math-optimizations",
        "-fno-omit-frame-pointer",
        "-Wall",
        "-Wextra",
        "-Wno-unused-parameter",
        "-march=nehalem",
        "-municode"
        // "-L${RACK_DIR}",
        // "-lRack",
        // "-static-libstdc++",
      ],
      "browse": {
        "databaseFilename": "${workspaceFolder}\\.vscode\\browse.vc.db",
        "limitSymbolsToIncludedHeaders": true,
        "path": [
          "${default}"
        ]
      },
      "compilerPath": "C:\\msys64\\mingw64\\bin\\g++.exe",
      "intelliSenseMode": "windows-gcc-x64",
      "cStandard": "gnu11",
      "cppStandard": "gnu++11",
      "configurationProvider": "ms-vscode.makefile-tools",
      "mergeConfigurations": true
    }
  ]
}
